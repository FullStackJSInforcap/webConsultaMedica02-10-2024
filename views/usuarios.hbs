    <div class="container mt-5">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">id</th>
                    <th scope="col">nombre</th>
                    <th scope="col">apellido</th>
                    <th scope="col">fecha</th>
                    <th scope="col">hora</th>
                    <th scope="col">g√©nero</th>
                </tr>
            </thead>
            <tbody>
                {{#each usuarios}}
                <tr>
                    <td>{{this._id}}</td>
                    <td>{{this._nombre}}</td>
                    <td>{{this._apellido}}</td>
                    <td>{{this._fecha}}</td>
                    <td>{{this._hora}}</td>
                    <td>{{this._genero}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
